---
import {IndexPostsContainer, IndexWelcome} from "@components/index"

const {posts} = Astro.props;
---

<div class="solitude-index-main grid grid-cols-12 pb-10 content-end">
    <IndexWelcome/>
    <IndexPostsContainer posts={posts}/>
</div>

<style>
    .solitude-index-main {
        height : 75vh;
    }
</style>
